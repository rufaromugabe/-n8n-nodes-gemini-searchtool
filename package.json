{
  "name": "n8n-nodes-gemini-searchtool",
  "version": "0.1.3",
  "description": "n8n node acting as a Google Search tool powered by the Google Gemini API.",
  "main": "dist/GeminiSearchTool.node.js",
  "types": "dist/GeminiSearchTool.node.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build"
  },
  "files": [
    "dist/*"
  ],
  "author": "Rufaro Mugabe rufarorevmugabe@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rufaromugabe/n8n-nodes-gemini-searchtool.git"
  },
  "keywords": [
    "n8n",
    "custom-node",
    "google",
    "gemini",
    "ai",
    "agent",
    "tool",
    "search",
    "grounding"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/GoogleGeminiApi.credentials.js"
    ],
    "nodes": [
      {
        "filePath": "dist/GeminiSearchTool.node.js",
        "name": "geminiSearchTool",
        "displayName": "Gemini Google Search Tool",
        "description": "Uses Google Gemini API to perform a grounded Google Search"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/request-promise-native": "^1.0.21",
    "n8n-workflow": "^1.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "dependencies": {
    "request-promise-native": "^1.0.9"
  }
}
